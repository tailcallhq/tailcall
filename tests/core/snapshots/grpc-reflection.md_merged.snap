---
source: tests/core/spec.rs
expression: formatter
---
schema
  @server
  @upstream(httpCache: 42)
  @link(src: "config.yml", type: Config)
  @link(src: "http://localhost:50051", type: Grpc) {
  query: Query
}

enum Status {
  DELETED
  DRAFT
  PUBLISHED
}

type News {
  body: String
  id: Int
  postImage: String
  status: Status
  title: String
}

type NewsData {
  news: [News]
}

type Query {
  news: NewsData! @grpc(url: "http://localhost:50051", method: "news.NewsService.GetAllNews")
}
